{
	"name": "nmyv",
	"version": "0.1",
	"description": "A quick extension for interacting with youtube-viewer",
	"permissions": ["activeTab", "tabs", "nativeMessaging", "contextMenus"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "nmyv_128.png"
	},
	"icons": {
		"16": "nmyv_16.png",
		"48": "nmyv_48.png",
		"128": "nmyv_128.png"
	},
	"manifest_version": 2
}
